<Alloy>
	<Window id="frmWin" class="container" title="Keyboard Behavior">
		<RightNavButton platform="ios">
			<Button image="common/allsize/cmd16.png" onClick="showSource" />
		</RightNavButton>
		<Menu platform="android">
			<MenuItem title="Source" icon="common/allsize/cmd16.png" onClick="showSource" />
		</Menu>
		<Label id="lblInfo" />
		<ListView id="listView">
			<Templates>
				<ItemTemplate name="myCell" height="50">
					<TextField bindId="bindField" class="bindField" />
				</ItemTemplate>
			</Templates>
			<ListSection id="listSection">
			</ListSection>
		</ListView>
	</Window>
</Alloy>