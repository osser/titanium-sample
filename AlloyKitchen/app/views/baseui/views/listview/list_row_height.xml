<Alloy>
	<Window class="container" title="Row Height">
		<RightNavButton platform="ios">
			<Button image="common/allsize/cmd16.png" onClick="showSource" />
		</RightNavButton>
		<Menu platform="android">
			<MenuItem title="Source" icon="common/allsize/cmd16.png" onClick="showSource" />
		</Menu>
		<ListView id="listView">
			<Templates>
				<ItemTemplate name="myCell" height="60">
					<Label bindId="cellLabel" class="cellLabel"></Label>
				</ItemTemplate>
			</Templates>
			<ListSection id="listSection"></ListSection>
		</ListView>
	</Window>
</Alloy>